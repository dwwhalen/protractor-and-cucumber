{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug via Protractor",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/protractor/built/cli.js",
            "stopOnEntry": false,
            "args": [
                "${workspaceRoot}/protractor.conf.js",
                "--browser=chrome"
            ]
        }
    ]
}